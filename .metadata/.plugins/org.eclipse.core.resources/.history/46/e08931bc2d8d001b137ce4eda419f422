package com.ilan;

import java.util.Arrays;
import java.util.stream.Collectors;

public class MultiInputString {

	public static void main(String[] args) {

		MultiInputString m = new MultiInputString();

		System.out.println(">>>>>"+m.alpha("One", "Two"));

//		String arr[] = { "Three", "Four" };
//
//		System.out.println(m.alpha(arr));

	}

	public String alpha(String... input) {
		String arr[] = input;
		String all = "";
		 Arrays.stream(arr).map(e -> all.concat(" ; " + e))
				//.peek(System.out::println)
				.collect(Collectors.toList());
				
				return null;
	}

}
