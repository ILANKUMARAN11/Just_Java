package com.ilan;

import java.util.Arrays;
import java.util.stream.Collectors;

public class MultiInputString {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

alpha("test","one");
		
		alpha(args);
		
	}
	
	public static String alpha(String... input) {
		String arr[]=input;
		String all="";
		Arrays.stream(arr).map(e->all.concat(" ; "+e)).collect(Collectors.toList());
		return all;
	}

}
