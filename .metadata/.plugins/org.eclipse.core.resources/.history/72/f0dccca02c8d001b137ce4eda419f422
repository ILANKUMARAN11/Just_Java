package com.ilan;

import java.util.Arrays;
import java.util.stream.Collectors;

public class MultiInputString {

	public static void main(String[] args) {

		System.out.println(alpha("test", "one"));

		System.out.println(alpha({"test", "one"}));

	}

	public static String alpha(String... input) {
		String arr[] = input;
		String all = "";
		Arrays.stream(arr).map(e -> all.concat(" ; " + e)).collect(Collectors.toList());
		return all;
	}

}
